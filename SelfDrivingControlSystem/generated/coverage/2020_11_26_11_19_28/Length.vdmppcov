
class Length

	types 
		public length_t = nat;
		
	instance variables
		private length_: length_t;
		
	inv length_ > 0;
	
		
	operations
	
		public Length: length_t ==> Length
			Length(length) == length_ := length
			pre length > 0
			post length_ = length;
		
		pure public getLength: () ==> length_t		/* pure is when you don't touch the object, only gets from it */
			getLength() == return length_
			post RESULT = length_;

end Length