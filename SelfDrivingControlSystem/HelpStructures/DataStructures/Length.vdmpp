
class Length

	types 
		public length_t = nat;
		
	instance variables
		private length: length_t;
		
	inv length > 0;
	
		
	operations
	
		public Length: length_t ==> Length
			Length(le) == length := le
			pre le > 0
			post length = le;
		
		pure public getLength: () ==> length_t		/* pure is when you don't touch the object, only gets from it */
			getLength() == return length
			post RESULT = length;

end Length